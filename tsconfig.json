{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "noImplicitAny": true,
    "lib": [ "es2015", "dom", "dom.iterable" ]
  },
  "include": ["./lib/*"],
  "exclude": ["node_modules"]
}